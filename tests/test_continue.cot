// Test continue in while loop
// expected: 42
fn main() int {
    var i = 0;
    var sum = 0;
    while (i < 10) {
        i = i + 1;
        if (i == 5) continue;  // skip adding 5
        sum = sum + i;
    }
    // sum should be 1+2+3+4 + 6+7+8+9+10 = 50 (skipping 5)
    if (sum != 50) return sum;
    return 42;
}
